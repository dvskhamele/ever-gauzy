{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "dist/apps/gauzy/**",
          "dist/packages/**",
          "package.json",
          "yarn.lock",
          "vercel.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/apps/gauzy/server.js"
    }
  ]
}